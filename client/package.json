    "@types/node": "10.12.18",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/yup": "^0.26.10",
    "apollo": "^2.5.1",
    "prettier": "^1.16.4",
    "react-scripts": "4.0.3",
    "typescript": "4.3.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
