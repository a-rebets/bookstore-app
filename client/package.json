{
  "name": "bookstore-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gql:schema": "apollo service:download -c apollo.config.js",
    "gql:types": "apollo client:codegen -c apollo.config.js --localSchemaFile=schema.graphql --target=typescript --globalTypesFile=src/graphql-types.ts",
    "format": "prettier --write 'src/**/*.ts*'"
  },
  "resolutions": {
    "graphql": "15.5.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-link": "^1.2.8",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-ws": "^1.0.14",
    "classnames": "^2.2.6",
    "formik": "^1.5.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "mobx": "^4.9.2",
    "mobx-react": "^5.4.3",
    "mobx-state-router": "^4.0.3",
    "react": "^16.8.3",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "^16.8.3",
    "subscriptions-transport-ws": "^0.9.15",
